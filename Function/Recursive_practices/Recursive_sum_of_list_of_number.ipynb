   ],
   "source": [
    "def listnum(numlist):\n",
    "    if len(numlist)==1:\n",
    "        return numlist[0]\n",
    "    else:\n",
    "        return numlist[0]+listnum(numlist[1:])\n",
    "print(listnum([2,3,5,9,6]))"
   ]
